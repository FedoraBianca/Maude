fmod LIST-INT2 is
  protecting INT .
  sorts NList List .
  subsort Int < NList < List .
  op nil : -> List .
  op _ _ : Int List -> NList [id: nil] .
endfm

fmod TREE is
  protecting INT .
  protecting LIST-INT2 .
  sort Tree .
  op empty : -> Tree .
  op _ _ _ : Tree Int Tree -> Tree .
  op mirror(empty) = empty .
  op mirror(L I R) = mirror(R) I mirror (L) .
  vars S D : Tree .
  vars X R : Int .
endfm

red mirror((empty 3 ( empty 1 empty )) 5 ((empty 6 empty) 2 empty) .
